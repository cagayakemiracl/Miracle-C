AR = /usr/bin/ar
#RANLIB = /usr/bin/ranlib
CC = gcc
#CC = gcc-4.2
#CC = clang

CFLAGS = -W -Wall -O3
#CFLAGS = -W -Wall -g
#CFLAGS = -W -Wall -O4

.SUFFIXES: .c .o

all: $(TARGET)

$(TARGET): $(OBJS) 
	$(AR) r $(TARGET) $(OBJS)
	cp $(TARGET) ../../.

%.o : %.c
	$(CC) $(CFLAGS) -c $*.c

clean:
	rm -rf $(OBJS) $(TARGET) *~ ../include/*~ ../../$(TARGET)
